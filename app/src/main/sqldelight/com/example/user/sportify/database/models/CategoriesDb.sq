CREATE TABLE categories (
    _ID INTEGER AS Integer NOT NULL PRIMARY KEY AUTOINCREMENT,
    category_id INTEGER AS Integer NOT NULL,
    name TEXT NOT NULL,
    image_url TEXT NOT NULL

);

insert_category:
INSERT OR REPLACE INTO categories(category_id, name, image_url)
VALUES (?, ?, ?);

select_all:
SELECT *
FROM categories;

delete_table:
DROP TABLE IF EXISTS categories;